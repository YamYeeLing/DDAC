@model IEnumerable<DDAC.Models.CargoBooking>

@{
    ViewBag.Title = "ViewBooking";
}

<h2>ViewBooking</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AgentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cargo.CargoName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shiptransaction.ShipName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AgentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo.CargoName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Shiptransaction.ShipName)
            </td>
        </tr>
    }

</table>
